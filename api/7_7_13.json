{
	"publicAPI":
	{
		"publish_settings":
		{
			"formats": [ "all" ],
			"params": {},
			"description": "Publishes current settings of the channel."
		},
		
		"publish_health":
		{
			"formats": [ "all" ],
			"params": {},
			"description": "Publishes current health stats of the channel."
		},
		
		"publish_api":
		{
			"formats": [ "all" ],
			"params": {},
			"description": "Publishes the API of the channel."
		},
		
		"video_start":
		{
			"formats": [ "all" ],
			"params": {},
			"description": "Starts the video stream."
		},
		
		"video_stop":
		{
			"formats": [ "all" ],
			"params": {},
			"description": "Stops the video stream."
		},
		
		"force_iframe":
		{
			"formats": [ "h264" ],
			"params": {},
			"description": "Forces the channel to output an I-frame."
		},
		
		"apply_settings":
		{
			"formats": [ "all" ],
			"params": 
			{
				"settings":
				{
					"type": "object",
					"description": "Collection of objects describing specific settings. See individual settings API for more info."
				}
			},
			"description": "Allows the user to set multiple settings at once."
		}
	},
	
	"settingsAPI":
	{
		"framerate":
		{
			"formats": [ "all" ],
			"params": 
			{
				"value":
				{
					"type": "uint32",
					"unit": "fps",
					"min": 1,
					"max": 60,
					"description": "Sets max framerate of the channel. Does not guarantee the true framerate."
				}
			}
		},
		
		"bitrate":
		{
			"formats": [ "all" ],
			"params": 
			{
				"value":
				{
					"type": "uint32",
					"unit": "bps",
					"min": 1000,
					"max": 10000000,
					"description": "Sets max bitrate of the channel. Does not guarantee the true bitrate."
				}
			}
		},
		
		"goplen":
		{
			"formats": [ "h264" ],
			"params": 
			{
				"value":
				{
					"type": "uint32",
					"unit": "frames",
					"min": 1,
					"max": 300,
					"description": "Number of P-frames between each I-frame."
				}
			}
		},
		
		"gop_hierarchy_level":
		{
			"formats": [ "h264" ],
			"params": 
			{
				"value":
				{
					"type": "uint32",
					"unit": "level",
					"min": 0,
					"max": 4,
					"description": "GOP Hierarchy Level."
				}
			}
		},
		
		"avc_profile":
		{
			"formats": [ "h264" ],
			"params": 
			{
				"value":
				{
					"type": "string",
					"unit": "",
					"options": [ "baseline", "main", "high" ],
					"description": "AVC Encoding Profile."
				}
			}
		},
		
		"avc_level":
		{
			"formats": [ "h264" ],
			"params": 
			{
				"value":
				{
					"type": "uint32",
					"unit": "level",
					"min": 10,
					"max": 52,
					"description": "AVC Encoding Level."
				}
			}
		},
		
		"maxnal":
		{
			"formats": [ "h264" ],
			"params": 
			{
				"enabled":
				{
					"type": "bool",
					"description": "When enabled, caps the max size of a given NAL unit to the specified value. If a frame is larger than this value, it will be broken up into multiple NAL units. If false, value will be implicitly set to 0."
				},
				
				"value":
				{
					"type": "uint32",
					"unit": "bytes",
					"min": 1,
					"max": 500000,
					"description": "Max NAL size in bytes."
				}
			}
		},
		
		"vui":
		{
			"formats": [ "h264" ],
			"params": 
			{
				"enabled":
				{
					"type": "bool",
					"description": "When enabled, includes VUI information in the h264 SPS."
				}
			}
		},
		
		"pict_timing":
		{
			"formats": [ "h264" ],
			"params": 
			{
				"enabled":
				{
					"type": "bool",
					"description": "When enabled, includes picture timing information in the h264 SPS."
				}
			}
		},
		
		"max_framesize":
		{
			"formats": [ "h264" ],
			"params": 
			{
				"value":
				{
					"type": "uint32",
					"unit": "bytes",
					"min": 1,
					"max": 500000,
					"description": "Max frame size in bytes."
				}
			}
		},
		
		"compression_quality":
		{
			"formats": [ "mjpeg" ],
			"params": 
			{
				"value":
				{
					"type": "uint32",
					"unit": "bytes",
					"min": 0,
					"max": 10000,
					"description": "Sets MJPEG compression quality in terms of image quantization parameter (QP)."
				}
			}
		},
		
		"flip_vertical":
		{
			"formats": [ "all" ],
			"params": 
			{
				"enabled":
				{
					"type": "bool",
					"description": "Flips image vertically."
				}
			}
		},
		
		"flip_horizontal":
		{
			"formats": [ "all" ],
			"params": 
			{
				"enabled":
				{
					"type": "bool",
					"description": "Flips image horizontally."
				}
			}
		},
		
		"contrast":
		{
			"formats": [ "all" ],
			"params": 
			{
				"value":
				{
					"type": "uint16",
					"unit": "none",
					"min": 0,
					"max": 200,
					"description": "Image contrast."
				}
			}
		},
		
		"zoom":
		{
			"formats": [ "all" ],
			"params": 
			{
				"value":
				{
					"type": "uint16",
					"unit": "none",
					"min": 0,
					"max": 100,
					"description": "Image zoom."
				}
			}
		},
		
		"pan":
		{
			"formats": [ "all" ],
			"params": 
			{
				"value":
				{
					"type": "int32",
					"unit": "none",
					"min": -648000,
					"max": 648000,
					"description": "Image pan."
				}
			}
		},
		
		"tilt":
		{
			"formats": [ "all" ],
			"params": 
			{
				"value":
				{
					"type": "int32",
					"unit": "none",
					"min": -648000,
					"max": 648000,
					"description": "Image tilt."
				}
			}
		},
		
		"pantilt":
		{
			"formats": [ "all" ],
			"params": 
			{
				"pan":
				{
					"type": "int32",
					"unit": "none",
					"min": -648000,
					"max": 648000,
					"description": "Image pan."
				},
				
				"tilt":
				{
					"type": "int32",
					"unit": "none",
					"min": -648000,
					"max": 648000,
					"description": "Image tilt."
				}
			}
		},
		
		"brightness":
		{
			"formats": [ "all" ],
			"params": 
			{
				"value":
				{
					"type": "int16",
					"unit": "none",
					"min": -255,
					"max": 255,
					"description": "Image brightness."
				}
			}
		},
		
		"hue":
		{
			"formats": [ "all" ],
