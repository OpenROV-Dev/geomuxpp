					"max": 18000,
					"description": "Image hue."
				}
			}
		},
		
		"gamma":
		{
			"formats": [ "all" ],
			"params": 
			{
				"value":
				{
					"type": "uint16",
					"unit": "none",
					"min": 100,
					"max": 300,
					"description": "Image gamma."
				}
			}
		},
		
		"saturation":
		{
			"formats": [ "all" ],
			"params": 
			{
				"value":
				{
					"type": "uint16",
					"unit": "none",
					"min": 0,
					"max": 200,
					"description": "Image saturation."
				}
			}
		},
		
		"gain":
		{
			"formats": [ "all" ],
			"params": 
			{
				"value":
				{
					"type": "uint16",
					"unit": "none",
					"min": 1,
					"max": 100,
					"description": "Image gain."
				}
			}
		},
		
		"sharpness":
		{
			"formats": [ "all" ],
			"params": 
			{
				"value":
				{
					"type": "uint16",
					"unit": "none",
					"min": 1,
					"max": 100,
					"description": "Image sharpness."
				}
			}
		},
		
		"max_analog_gain":
		{
			"formats": [ "all" ],
			"params": 
			{
				"value":
				{
					"type": "uint32",
					"unit": "none",
					"min": 0,
					"max": 15,
					"description": "Maximum sensor analog gain in auto exposure algorithm."
				}
			}
		},
		
		"histogram_eq":
		{
			"formats": [ "all" ],
			"params": 
			{
				"enabled":
				{
					"type": "bool",
					"description": "Enable/disable histogram equalization, which gives more contrast to the image."
				}
			}
		},
		
		"sharpen_filter":
		{
			"formats": [ "all" ],
			"params": 
			{
				"value":
				{
					"type": "uint32",
					"unit": "none",
					"min": 0,
					"max": 2,
					"description": "Strength of the sharpening filter, higher being stronger."
				}
			}
		},
		
		"min_exp_framerate":
		{
			"formats": [ "all" ],
			"params": 
			{
				"value":
				{
					"type": "uint32",
					"unit": "none",
					"min": 0,
					"max": 30,
					"description": "Minimum framerate that the autoexposure algorithm can drop to."
				}
			}
		},
		
		"tf_strength":
		{
			"formats": [ "all" ],
			"params": 
			{
				"enabled":
				{
					"type": "bool",
					"description": "Enable/disable the temporal filter."
				},
				
				"value":
				{
					"type": "uint32",
					"unit": "none",
					"min": 1,
					"max": 7,
					"description": "Strength of the temporal filter."
				}
			}
		},
		
		"gain_multiplier":
		{
			"formats": [ "all" ],
			"params": 
			{
				"value":
				{
					"type": "uint32",
					"unit": "none",
					"min": 0,
					"max": 256,
					"description": "Controls the autoexposure algorithm to adjust the sensor analog gain and exposure based on different lighting conditions."
				}
			}
		},
		
		"exp":
		{
			"formats": [ "all" ],
			"params": 
			{
				"mode":
				{
					"type": "string",
					"unit": "",
					"options": [ "auto", "manual" ],
					"description": "Autoexposure mode."
				},
				
				"value":
				{
					"type": "uint16",
					"unit": "none",
					"min": 0,
					"max": 255,
					"description": "Exposure time."
				}
			}
		},
		
		"nf":
		{
			"formats": [ "all" ],
			"params": 
			{
				"mode":
				{
					"type": "string",
					"unit": "",
					"options": [ "auto", "manual" ],
					"description": "Noise filter mode."
				},
				
				"value":
				{
					"type": "uint16",
					"unit": "none",
					"min": 0,
					"max": 100,
					"description": "Noise filter strength."
				}
			}
		},
		
		"wb":
		{
			"formats": [ "all" ],
			"params": 
			{
				"mode":
				{
					"type": "string",
					"unit": "",
					"options": [ "auto", "manual" ],
					"description": "White balance mode."
				},
				
				"value":
				{
					"type": "uint16",
					"unit": "none",
					"min": 2800,
					"max": 6500,
					"description": "White balance temperature."
				}
			}
		},
		
		"wdr":
		{
			"formats": [ "all" ],
			"params": 
			{
				"enabled":
				{
					"type": "bool",
					"description": "Enable/disable Wide Dynamic Range mode."
				},
				
				"mode":
				{
					"type": "string",
					"unit": "",
					"options": [ "auto", "manual" ],
					"description": "WDR mode."
				},
				
				"value":
				{
					"type": "uint8",
					"unit": "none",
					"min": 0,
					"max": 255,
					"description": "WDR intensity."
				}
			}
		},
		
		"zone_exp":
		{
			"formats": [ "all" ],
			"params": 
			{
				"enabled":
				{
					"type": "bool",
